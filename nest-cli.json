{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/validator/src",
  "monorepo": true,
  "root": "apps/http-validator",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/http-validator/tsconfig.app.json"
  },
