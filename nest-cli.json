      "type": "application",
      "root": "apps/network-validator",
      "entryFile": "main",
      "sourceRoot": "apps/network-validator/src",
      "compilerOptions": {
        "plugins": [
          {
            "name": "@nestjs/swagger",
            "options": {
              "dtoFileNameSuffix": [".dto.ts", ".schema.ts"]
