  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest ",
    "test:e2e": "jest -c jest.e2e.ts --runInBand --coverage --detectOpenHandles --forceExit",
    "test:e2e-ci": "jest -c jest.e2e.ts --runInBand --coverage --detectOpenHandles --forceExit --ci --reporters=default --reporters=jest-junit",
    "test:e2e-persist": "jest -c ./apps/persist/test/jest-e2e.ts --detectOpenHandles --forceExit",
    "test:e2e-parse": "jest -c ./apps/parse/test/jest-e2e.ts --detectOpenHandles --forceExit",
    "test:e2e-wallet": "jest -c ./apps/wallet/test/jest-e2e.ts --detectOpenHandles --forceExit",
    "test:e2e-http-gateway": "jest -c ./apps/http-gateway/test/jest-e2e.ts --detectOpenHandles --forceExit",
    "test:e2e-http-observer": "jest -c ./apps/http-observer/test/jest-e2e.ts --detectOpenHandles --forceExit",
    "test:e2e-http-validator": "jest -c ./apps/http-validator/test/jest-e2e.ts --detectOpenHandles --forceExit",
    "test:e2e-network-gateway": "jest -c ./apps/network-gateway/test/jest-e2e.ts --detectOpenHandles --forceExit",
    "test:e2e-network-observer": "jest -c ./apps/network-observer/test/jest-e2e.ts --detectOpenHandles --forceExit",
    "test:e2e-network-validator": "jest -c ./apps/network-validator/test/jest-e2e.ts --detectOpenHandles --forceExit",
    "test:e2e-persist-ci": "jest -c ./apps/persist/test/jest-e2e.ts --detectOpenHandles --forceExit --ci --reporters=default --reporters=jest-junit",
    "test:e2e-parse-ci": "jest -c ./apps/parse/test/jest-e2e.ts --detectOpenHandles --forceExit --ci --reporters=default --reporters=jest-junit",
    "test:e2e-wallet-ci": "jest -c ./apps/wallet/test/jest-e2e.ts --detectOpenHandles --forceExit --ci --reporters=default --reporters=jest-junit",
    "test:e2e-http-gateway-ci": "jest -c ./apps/http-gateway/test/jest-e2e.ts --detectOpenHandles --forceExit --ci --reporters=default --reporters=jest-junit",
    "test:e2e-http-observer-ci": "jest -c ./apps/http-observer/test/jest-e2e.ts --detectOpenHandles --forceExit --ci --reporters=default --reporters=jest-junit",
    "test:e2e-http-validator-ci": "jest -c ./apps/http-validator/test/jest-e2e.ts --detectOpenHandles --forceExit --ci --reporters=default --reporters=jest-junit",
    "test:e2e-network-gateway-ci": "jest -c ./apps/network-gateway/test/jest-e2e.ts --detectOpenHandles --forceExit --ci --reporters=default --reporters=jest-junit",
    "test:e2e-network-observer-ci": "jest -c ./apps/network-observer/test/jest-e2e.ts --detectOpenHandles --forceExit --ci --reporters=default --reporters=jest-junit",
    "test:e2e-network-validator-ci": "jest -c ./apps/network-validator/test/jest-e2e.ts --detectOpenHandles --forceExit --ci --reporters=default --reporters=jest-junit",
    "test:network": "jest -c ./test/network/jest.network.ts --coverage",
    "test:unit": "jest -c jest.unit.ts --coverage",
    "test:unit-ci": "jest -c jest.unit.ts --coverage --ci --reporters=default --reporters=jest-junit",
    "test:unit-persist": "jest ./apps/persist/src",
    "doc": "npx compodoc -p tsconfig.json -w --theme material -s -r 8000",
    "doc:build": "npx compodoc -p tsconfig.json",
    "watch:wallet": "nest build wallet --watch",
    "watch:parse": "nest build parse --watch",
    "watch:persist": "nest build persist --watch",
    "watch:http-gateway": "nest build http-gateway --watch",
    "watch:http-observer": "nest build http-observer --watch",
    "watch:http-validator": "nest build http-validator --watch",
    "watch:network-gateway": "nest build network-gateway --watch",
    "watch:network-observer": "nest build network-observer --watch",
    "watch:network-validator": "nest build network-validator --watch"
  },
